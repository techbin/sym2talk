{
  "expo": {
    "name": "sym2talk",
    "slug": "sym2talk",
    "version": "1.0.1",
    "orientation": "landscape",
    "icon": "./assets/wechat.png",
    "splash": {
      "image": "./assets/wechat.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "sym2talk"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/wechat.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.satish.prg.sym2talk",
      "permissions": ["MANAGE_DOCUMENTS", "READ_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/wechat.png"
    },
    "description": "Sym2Talk is a Communication App, also known as AAC App meant for the adults who have difficulty using speech to convey their thoughts and ideas using 8000 images.\n\nThis Communication Apps, also known as AAC App has been tested on both iOS and Android phones, tablets etc. "
  }
}
